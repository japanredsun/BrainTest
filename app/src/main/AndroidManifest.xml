<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jrgroup.braintest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TrainingActivity"
            android:exported="true" />
        <activity android:name=".RecordActivity" />
        <activity android:name=".DailyTestActivity" />
        <activity
            android:name=".ReflexionGameActivity"
            android:exported="true" />
        <activity android:name=".QuickMathActivity" />
        <activity android:name=".VisionActivity" android:exported="true"></activity>
        <service android:enabled="true" android:name="BackgroundSoundService" />
    </application>


</manifest>